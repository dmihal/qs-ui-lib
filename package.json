{
  "name": "qs-ui-library",
  "description": "Quantstamp's canonical UI-Library",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://www.quantstamp.com",
  "author": {
    "name": "Jeff Scott Ward",
    "url": "https://www.twitter.com/jeffscottward"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/quantstamp/qs-ui-library.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "webpack",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "styleguide:deploy": "npm run styleguide:build && cd styleguide && now && cd ..",
    "start": "micro",
    "dev": "scripts/dev.sh"
  },
  "postcss": {
    "plugins": {
      "postcss-nested": "^3.0.0",
      "postcss-media-minmax": "^@3.0.0"
    }
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      [
        "styled-jsx/babel",
        {
          "plugins": [
            "styled-jsx-plugin-postcss"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "codemirror": "^5.39.2",
    "css-loader": "^0.28.11",
    "dog-names": "^1.0.2",
    "file-loader": "^1.1.11",
    "jss-cli": "^5.0.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "nuka-carousel": "^4.2.1",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nested": "^3.0.0",
    "print-env": "^0.1.2",
    "prop-types": "^15.6.1",
    "rc-slider": "^8.6.1",
    "rc-tooltip": "^3.7.2",
    "react": "^16.4.0",
    "react-codemirror2": "^5.1.0",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.2.0",
    "react-form": "^3.5.5",
    "react-select": "^1.2.1",
    "react-slick": "^0.23.1",
    "sanitize.css": "^5.0.0",
    "shelljs": "^0.8.2",
    "style-loader": "^0.20.3",
    "styled-jsx": "^2.2.6",
    "styled-jsx-plugin-postcss": "^0.1.3",
    "to-string-loader": "^1.1.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "cypress": "^3.0.2",
    "gulp-babel": "^7.0.1",
    "gulp-shell": "^0.6.5",
    "html-webpack-plugin": "^3.2.0",
    "react-styleguidist": "^7.0.17",
    "semantic-release": "^15.8.1",
    "webpack": "^4.12.0"
  }
}
